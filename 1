<body>
    <script>
        function processConfirm(question) {
            let answer = confirm(question);
            let result = '';
            if (answer) {
                result += "Excellent. We'll play a nice game of chess.";
            } else {
                result += 'Maybe later then.';
            }
            return result;
        }
        let message = processConfirm('Bạn có chắc chắn với quyết định của mình không');
        console.log(message); 
    </script>
</body>
</html>
